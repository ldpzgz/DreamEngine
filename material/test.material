vs:1{
	#version 300 es
    layout(location = 0) in vec4 a_position;
    layout(location = 1) in vec2 a_texCoord;
    out vec2 v_texCoord;
    void main()
    {
       gl_Position = a_position;
       v_texCoord = a_texCoord;
    }
}

fs:1{
	#version 300 es
	precision mediump float;
	in vec2 v_texCoord;
	layout(location = 0) out vec4 outColor;
	uniform sampler2D s_texture;
	void main()
	{
	  outColor = texture( s_texture, v_texCoord );
	}
}

texture:test0{
	path=./opengles3/images/buti.jpg
}

texture:test1{
	width=640
	height=480
	depth=3
}

program:basic{
	vs=1
	fs=1
	posLoc=0
	texcoordLoc=1
	sampler2D{
		s_texture=test0
	}
}



