<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	id="llayout"
	width="matchParent"
	height="40"
	gravity="leftCenter"
	background="#000000ff"
	orientation="h">
	<TextView
		id="wrapIcon"
		width="16"
		height="16"
		gravity="center"
		background="@image/ui/wrap.png"
	/>
	<TextView
		id="objectIcon"
		width="32"
		height="32"
		gravity="center"
		background="@image/ui/node.png"
	/>
	<TextView
		id="tv"
		width="160"
		height="32"
		gravity="leftCenter"
		textSize="32"
		textColor="#ffffffff"
	/>
</LinearLayout>